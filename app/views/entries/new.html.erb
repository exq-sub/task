<div class="users-new-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-4 users-new-container">
        <h1 class="text-center text-white">Entry Sheet</h1>
<div>
  <%= form_for @entry, url: entries_path do |f| %>
  
  
   <div class="form-group">
      <%= f.label :name, class: 'text-white' %>
      <%= f.text_field :name, class: 'form-control' %>
   </div>
  
   <div class="form-group">
      <%= f.label :message, class: 'text-white' %>
      <%= f.text_field :message, class: 'form-control' %>
   </div>
  
    <%= f.submit '送信' %>
  
  <% end %>
</div>
</div>
</div>
</div>
</div>